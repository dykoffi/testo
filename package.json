{
    "name": "testo",
    "version": "1.0.0",
    "main": "./index.ts",
    "private": false,
    "scripts": {
        "test": "jest",
        "start:dev": "nodemon ./index.ts",
        "start:debug": "DEBUG=* nodemon ./index.ts",
        "db:update": "prisma migrate dev && cqx update",
        "db:start": "docker-compose up -d testo_db_dev",
        "compose:up": "docker-compose up -d testo",
        "docker:build": "cqx build && DOCKER_BUILDKIT=1 docker build -t dykoffi/testo ./build && rm -rdf build"
    },
    "dependencies": {
        "@prisma/client": "3.12.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cqx-secure": "^1.0.12",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "prom-client": "^14.0.1",
        "serve-favicon": "^2.5.0",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/http-errors": "^1.8.2",
        "@types/jest": "^27.4.1",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.24",
        "@types/on-finished": "^2.3.1",
        "@types/serve-favicon": "^2.5.3",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.19.1-alpha.15",
        "@typescript-eslint/parser": "^5.19.1-alpha.15",
        "eslint": "^8.13.0",
        "jest": "^27.5.1",
        "prisma": "^3.12.0",
        "prisma-json-schema-generator": "^2.0.10",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3",
        "webpack": "^5.72.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.test\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "author": {
        "email": "koffiedy@gmail.com",
        "name": "KOFFI Edy",
        "url": "https://www.linkedin.com/in/edy-koffi-948494192/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dykoffi/testo.git"
    },
    "bugs": {
        "url": "https://github.com/dykoffi/testo/issues"
    },
    "homepage": "git+https://github.com/dykoffi/testo#readme",
    "license": "MIT"
}